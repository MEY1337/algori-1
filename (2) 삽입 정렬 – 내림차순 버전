# 삽입 정렬 (내림차순)
def insertion_sort_desc(a):
    n = len(a)
    for i in range(1, n):
        key = a[i]
        j = i - 1
        # 오름차순: a[j] > key → 내림차순: a[j] < key
        while j >= 0 and a[j] < key:
            a[j + 1] = a[j]
            j -= 1
        a[j + 1] = key
    return a

# 실행 예시
data = [2, 4, 5, 1, 3]
insertion_sort_desc(data)
print(data)  # [5, 4, 3, 2, 1]
